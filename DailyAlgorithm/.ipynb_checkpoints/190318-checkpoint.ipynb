   "source": [
    "## 정렬되지 않은 배열과 정수 n이 주어지면, 배열 안에 합이 N이 되는 두 원소를 찾으시오."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "0 8\n",
      "1 7\n",
      "2 6\n",
      "3 5\n"
     ]
    }
   ],
   "source": [
    "def test(a,n):\n",
    "    for i in range(0,len(a)-1):\n",
    "        for j in range(i+1,len(a)):\n",
    "            if i + j == n:\n",
    "                print(i,j)\n",
    "\n",
    "d = [0,10,8,7,2,3,6,4,1]\n",
    "test(d,8)"
   ]
  }
